{
  "family": "steadybit-extension-host",
  "containerDefinitions": [
    {
      "name": "steadybit-extension-host",
      "image": "ghcr.io/steadybit/extension-host:latest",
      "cpu": 205,
      "memory": 51,
      "portMappings": [],
      "essential": true,
      "environment": [
        {
          "name": "STEADYBIT_LOG_LEVEL",
          "value": "INFO"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "linuxParameters": {
        "capabilities": {
          "add": [
            "SYS_ADMIN",
            "SYS_CHROOT",
            "SYS_RESOURCE",
            "SYS_BOOT",
            "NET_RAW",
            "SYS_TIME",
            "SYS_PTRACE",
            "KILL",
            "NET_ADMIN",
            "DAC_OVERRIDE",
            "SETUID",
            "SETGID",
            "AUDIT_WRITE"
          ],
          "drop": []
        },
        "tmpfs": [
          {
            "containerPath": "/tmp",
            "size": 10,
            "mountOptions": [
              "exec"
            ]
          }
        ]
      },
      "readonlyRootFilesystem": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/steadybit-extension-host",
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "max-buffer-size": "25m",
          "awslogs-region": "MY-REGION",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "systemControls": []
    }
  ],
  "tags": [
    {
      "key": "steadybit_extension_type",
      "value": "ACTION:DISCOVERY"
    },
    {
      "key": "steadybit_extension_daemon",
      "value": "true"
    },
    {
      "key": "steadybit_extension_port",
      "value": "8085"
    }
  ],
  "networkMode": "host",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "EC2"
  ],
  "pidMode": "host",
  "ipcMode": "host",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}